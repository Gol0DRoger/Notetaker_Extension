{
    "manifest_version": 3,
    "name": "Notetaker",
    "description": "A simple notetaking Chrome extension with spell-check and local save.",
    "version": "1.0",
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "128": "icon.png"
      }
    },
    "icons": {
        "128": "icon.png"
      },
    "permissions": [
      "storage",
      "downloads"
    ]
  }
  